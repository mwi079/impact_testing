{
    "compilerOptions": {
      "target": "es6",
      "lib": [
        "es6"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "isolatedModules": true,
      "jsx": "preserve",
      "noImplicitAny": true,
      "strictNullChecks": false
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
    ]
  }